{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/userUpdate.schema.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "password": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "is_admin": {
      "type": "boolean"
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": []
}