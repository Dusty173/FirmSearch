{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/userUpdate.schema.json",
    "type": "object",
    "properties": {
      "aboutinfo": {
        "type": "string",
        "minLength": 1
      },
      "contact": {
        "type": "string",
        "minLength": 10,
        "maxLength": 15
      },
      "email": {
        "type": "string",
        "format": "email"
      }
    },
    "additionalProperties": false,
    "required": [
        "aboutinfo",
        "contact",
        "email"
    ]
  }